<h1>一覧</h1>

<%= link_to '新規作成', new_task_path %>

<%= @tasks.each do |task| %>
  <%= link_to task.user.name, user_path(task.user) %>
  <%= link_to task.name, task_path(task) %>
  <%= link_to '編集', edit_task_path(task) %>
  <%= link_to '削除', task_path(task), method: :delete, data: {confirm: "削除しますか？"} %>

<% end %>
